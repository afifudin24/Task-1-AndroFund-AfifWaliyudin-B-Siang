<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Garis horizontal atas -->
    <item android:left="-2dp" android:right="-2dp" android:top="-2dp">
        <shape android:shape="rectangle">
            <solid android:color="@android:color/transparent" />
            <stroke android:width="1dp" android:color="#000" />
        </shape>
    </item>
    <!-- Garis horizontal bawah -->
    <item android:left="-2dp" android:right="-2dp" android:bottom="-2dp">
        <shape android:shape="rectangle">
            <solid android:color="@android:color/transparent" />
            <stroke android:width="1dp" android:color="#000" />
        </shape>
    </item>
    <!-- Garis vertikal kiri -->
    <item android:left="-2dp" android:top="-2dp" android:bottom="-2dp">
        <shape android:shape="rectangle">
            <solid android:color="@android:color/transparent" />
            <stroke android:width="1dp" android:color="#000" />
        </shape>
    </item>
    <!-- Garis vertikal kanan -->
    <item android:right="-2dp" android:top="-2dp" android:bottom="-2dp">
        <shape android:shape="rectangle">
            <solid android:color="@android:color/transparent" />
            <stroke android:width="1dp" android:color="#000" />
        </shape>
    </item>
</layer-list>
